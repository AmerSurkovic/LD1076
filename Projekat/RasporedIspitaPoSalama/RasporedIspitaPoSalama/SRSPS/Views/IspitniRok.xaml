    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <StackPanel Name="stekPanelIspiti">
            <TextBlock Name="tBlockIspiti" Text="Ispiti"/>
            <ListView Name="listViewIspiti" HorizontalAlignment="Stretch" Background="Azure">
                <ListViewItem Content="Inženjerska matematika 2"/>
                <ListViewItem Content="Inženjerska fizika 2"/>
                <ListViewItem Content="Priroda i društvo"/>
                <ListViewItem Content="Embriologija"/>
